<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fiberbeamportal">

    <uses-permission android:name="android.permission.SEND_SMS"/>

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FiberBeamPortal">
        <activity android:name=".ShowUsersActivity" />
        <activity android:name=".FreeCustomer" />
        <activity android:name=".ViewBillsActivity" />
        <activity android:name=".AdminDashboard" />
        <activity android:name=".AddNewUser" />
        <activity android:name=".PayBill" />
        <activity android:name=".AddCustomer" />
        <activity android:name=".UserDashboard" />
        <activity android:name=".User" />
        <activity android:name=".Admin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>